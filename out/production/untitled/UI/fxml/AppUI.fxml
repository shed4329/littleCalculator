<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #8bc34a;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.controller.Controller">
   <children>
      <TextField fx:id="textFieldA" prefHeight="40.0" prefWidth="600.0" style="-fx-background-color: #e8f5e9;" />
      <TextField fx:id="textFieldB" prefHeight="48.0" prefWidth="600.0" style="-fx-background-color: #c8e6c9;" />
      <HBox prefHeight="237.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="237.0" prefWidth="212.0">
               <children>
                  <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#onButtonClick" onKeyPressed="#onButtonPressed" prefHeight="113.0" prefWidth="216.0" style="-fx-background-color: #81c784;" text="+" />
                  <Button fx:id="buttonSubtract" mnemonicParsing="false" onAction="#onButtonClick" onKeyPressed="#onButtonPressed" prefHeight="127.0" prefWidth="212.0" style="-fx-background-color: #66bb6a;" text="-" />
               </children>
            </VBox>
            <VBox prefHeight="237.0" prefWidth="213.0">
               <children>
                  <Button fx:id="buttonMultiplication" mnemonicParsing="false" onAction="#onButtonClick" onKeyPressed="#onButtonPressed" prefHeight="116.0" prefWidth="214.0" style="-fx-background-color: #66bb6a;" text="×" />
                  <Button fx:id="buttonDivide" mnemonicParsing="false" onAction="#onButtonClick" onKeyPressed="#onButtonPressed" prefHeight="125.0" prefWidth="213.0" style="-fx-background-color: #81c784;" text="÷" />
               </children>
            </VBox>
            <Button fx:id="buttonEqual" mnemonicParsing="false" onAction="#onButtonClick" onKeyPressed="#onButtonPressed" prefHeight="248.0" prefWidth="176.0" style="-fx-background-color: #4caf50;" text="=" />
         </children>
      </HBox>
      <Text fx:id="text" fill="#ee0b0b" strokeType="OUTSIDE" strokeWidth="0.0" text="结果">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</VBox>
